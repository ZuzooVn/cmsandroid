<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <declare-styleable name="UIButton">
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
        <attr name="image" format="integer" />
    </declare-styleable>
    <declare-styleable name="BlocksLayout">
        <attr name="columns" format="integer" />
    </declare-styleable>
    <declare-styleable name="TimeRulerView">
        <attr name="headerWidth" format="dimension" />
        <attr name="hourHeight" format="dimension" />
        <attr name="horizontalDivider" format="boolean" />
        <attr name="labelTextSize" format="dimension" />
        <attr name="labelPaddingLeft" format="dimension" />
        <attr name="labelColor" format="color" />
        <attr name="dividerColor" format="color" />
        <attr name="startHour" format="integer" />
        <attr name="endHour" format="integer" />
    </declare-styleable>
    <declare-styleable name="com.zia.freshdocs.widget.fileexplorer.CustomFastScrollView">
        <attr name="overlayWidth" format="dimension" />
        <attr name="overlayHeight" format="dimension" />
        <attr name="overlayTextSize" format="dimension" />
        <attr name="overlayScrollThumbWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="com.zia.freshdocs.widget.fileexplorer.Carousel">
        <attr name="android:gravity" />
        <attr name="android:animationDuration" />
        <attr name="UseReflection" format="boolean" />
        <attr name="Items" format="integer" />
        <attr name="SelectedItem" format="integer" />
        <attr name="maxTheta" format="float" />
        <attr name="minQuantity" format="integer" />
        <attr name="maxQuantity" format="integer" />
    </declare-styleable>

</resources>